{
    "extends": ["airbnb", "prettier"],
    "parser": "babel-eslint",
    "plugins": ["prettier"],
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": "warn",
      "no-unused-vars": "warn",
      "react/prop-types": "off"
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  }
  }
